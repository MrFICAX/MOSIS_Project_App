<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- log_in styles -->

	<style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>
	<style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
		<item name="cornerSize">30%</item>
	</style>
	<style name="photo">
		<item name="color">@color/black</item>
		<item name="cornerFamily">rounded</item>
		<item name="cornerSize">50%</item>
	</style>

	<style name="textStyle">
		<item name="strokeColor">@color/white</item>
		<item name="strokeWidth">3dp</item>
	</style>

	<style name="datePicker" parent="Theme.AppCompat.Light">
		<item name="colorAccent">@color/menuColor</item>
		<item name="colorControlActivated">#33691e</item>
		<item name="android:selectableItemBackgroundBorderless">@color/colorPrimaryDark</item>
		<item name="colorControlHighlight">#AA440000</item>
	</style>
	<style name="MenuItems" parent="AppTheme">
		<item name="android:textSize">28sp</item>
		<item name="android:fontFamily">sans-serif-light</item>
	</style>


</resources>